{
  "openapi": "3.0.0",
  "info": {
    "title": "Task API",
    "version": "1.0.0",
    "description": "A simple Task API"
  },
  "paths": {
    "/testjson/checkTasks": {
      "get": {
        "summary": "Get all tasks",
        "description": "Get all tasks",
        "tags": ["Task"],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/testjson/pushTasks": {
      "post": {
        "summary": "Push all tasks",
        "description": "Push all tasks",
        "tags": ["Task"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "$ref": "#/components/schemas/Task"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Task": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "task": {
            "type": "string"
          },
          "completed": {
            "type": "boolean"
          }
        }
      }
    }
  }
}
